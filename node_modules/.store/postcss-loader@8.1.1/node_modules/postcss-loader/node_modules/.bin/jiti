#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/blog/011/node_modules/.store/jiti@1.21.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/blog/011/node_modules/.store/jiti@1.21.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jiti@1.21.0/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../jiti@1.21.0/node_modules/jiti/bin/jiti.js" "$@"
fi
